{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "farm-vegetables",
  "compatibility_date": "2025-10-23",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "farm-vegetables-production",
      "database_id": "local-db-for-development"
    }
  ],
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "farm-vegetables-uploads"
    }
  ]
}